selenium:
  browser: Chrome
  minimize-browser: false
  hide-browser: false
  target-spider: cartoonmad
  ## random-interval > 0 : fix sleep interval between each download
  ## random-interval = 0 : no sleep, continue download
  ## random-interval < 0 : random sleep interval (from 1 ~ abs(random-interval) seconds) between each download
  random-interval: 0


